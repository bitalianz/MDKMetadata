									"EntitySet": "ZZDefectos",
									"QueryOptions": "$filter=OrderId eq '{{#Property:OrderId}}' and UserStatus eq '{{#Property:sUserStatusChar}}'",
									"Service": "/SAPAssetManager/Services/AssetManager.service"
								}
